<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<HBox spacing="10.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.flip.HelloController">
   <children>
      <Pane fx:id="pnJatek" prefHeight="532.0" prefWidth="788.0" style="-fx-background-color: white; -fx-border-color: gray;" />
      <VBox alignment="TOP_CENTER">
         <children>
            <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@icons/white.png" />
               </image>
            </ImageView>
            <Label fx:id="lbDarab" text="96db">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lbSzazalek" text="100%">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
            <ImageView fitHeight="64.0" fitWidth="64.0" onMousePressed="#onReload" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@icons/reload.png" />
               </image>
            </ImageView>
            <HBox alignment="CENTER">
               <children>
                  <ImageView fitHeight="32.0" fitWidth="32.0" onMousePressed="#onPrevLevel" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/prev32.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="lbLevel" text="1">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <ImageView fitHeight="32.0" fitWidth="32.0" onMousePressed="#onNextLevel" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/next32.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
