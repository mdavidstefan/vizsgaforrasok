<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csapat.HelloController">
   <children>
      <HBox spacing="10.0">
         <children>
            <Label style="-fx-border-color: black; -fx-border-width: 1px;">
               <graphic>
                  <ImageView fx:id="mainImg" fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Label>
            <VBox spacing="10.0">
               <children>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Név:" />
                        <Label fx:id="lbNev" textFill="BLUE">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Email:" />
                        <Label fx:id="lbEmail" textFill="BLUE">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label fx:id="lbRegisztralt" text="Regisztrált:" />
                        <Label fx:id="lbRegisztralt" textFill="BLUE">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <Button mnemonicParsing="false" onAction="#onKovetkezo" text="Következő" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox>
         <children>
            <Separator maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#felvesz1" prefWidth="130.0" text="Felvesz" />
            <Button mnemonicParsing="false" onAction="#felvesz2" prefWidth="130.0" text="Felvesz" />
            <Button mnemonicParsing="false" onAction="#felvesz3" prefWidth="130.0" text="Felvesz" />
         </children>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Label style="-fx-border-color: black; -fx-border-width: 1px;">
               <graphic>
                  <ImageView fx:id="img1" fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Label>
            <Label style="-fx-border-color: black; -fx-border-width: 1px;">
               <graphic>
                  <ImageView fx:id="img2" fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Label>
            <Label style="-fx-border-color: black; -fx-border-width: 1px;">
               <graphic>
                  <ImageView fx:id="img3" fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Label>
         </children>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Label fx:id="lbNev1" alignment="CENTER" prefWidth="130.0" textFill="BLUE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lbNev2" alignment="CENTER" prefWidth="130.0" textFill="BLUE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lbNev3" alignment="CENTER" prefWidth="130.0" textFill="BLUE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
