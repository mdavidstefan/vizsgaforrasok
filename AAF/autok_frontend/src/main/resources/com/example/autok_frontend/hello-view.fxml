<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox spacing="10.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.autok_frontend.HelloController">
   <children>
      <ComboBox fx:id="cmbxMarkak" onAction="#listaz" prefWidth="150.0" />
      <TableView fx:id="tvAutok">
        <columns>
          <TableColumn fx:id="tcMarka" prefWidth="100.0" text="Márka" />
          <TableColumn fx:id="tcTipus" prefWidth="100.0" text="Típus" />
            <TableColumn fx:id="tcEvjarat" prefWidth="50.0" text="Évjárat" />
            <TableColumn fx:id="tcUrl" prefWidth="450.0" text="Url" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
